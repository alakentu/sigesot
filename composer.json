{
    "name": "alakentu/helpdesk",
    "description": "SIGESOT, se encarga de organizar y gestionar las solicitudes de soporte técnico de los usuarios, facilitando la comunicación, seguimiento y resolución de problemas.",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Gonzalo Meneses",
            "email": "alakentu2003@hotmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alakentu/sigesot.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "codeigniter4/framework": "^4.6",
        "codeigniter4/translations": "^4.4",
        "predis/predis": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Config\\": "app/Config"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
